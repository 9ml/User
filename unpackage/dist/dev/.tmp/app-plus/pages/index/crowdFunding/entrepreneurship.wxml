<view><head-type-a vue-id="1" title="{{title}}" isWhite="{{isWhite}}" isHaveSpace="{{isHaveSpace}}" bind:__l="__l"></head-type-a><view class="topTypeMs"><block wx:for="{{topTypeMsList}}" wx:for-item="t" wx:for-index="t_idx" wx:key="t_idx"><view data-event-opts="{{[['tap',[['choseTTM',[t_idx]]]]]}}" class="{{['ttm-item',topTypeMsCurrent===t_idx?'ttm-ac':'']}}" bindtap="__e">{{''+t.name+''}}</view></block></view><view class="topTypeMsSpace"></view><block wx:for="{{itemList}}" wx:for-item="i" wx:for-index="i_idx" wx:key="i_idx"><view class="itemMs"><view class="im-image"><image src="{{i.image}}" mode></image></view><view class="im-main"><view class="imm-title">{{''+i.name+''}}</view><view class="imm-progress"><progress class="ip-main" show-info percent="{{i.percent}}" activeColor="#F77F0C" border-radius="10" backgroundColor="#E1E1E1" active stroke-width="20"></progress></view><view class="imm-info"><view class="immi-item"><view class="imi-top">已募集</view><view class="imi-bom">{{''+i.have+'元'}}</view></view><view class="immi-item"><view class="imi-top">目标金额</view><view class="imi-bom">{{''+i.all+'元'}}</view></view><view class="immi-item"><view class="imi-top">距离</view><view class="imi-bom">{{''+i.distance+''}}</view></view></view></view></view></block></view>