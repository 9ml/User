page {
  background-color: #FFFFFF;
}
.inpMs {
  margin-top: 40rpx;
}
.inpMs .im-item {
    width: 550rpx;
    margin: 20rpx auto 0;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    position: relative;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
    border-bottom: 1rpx solid #AEAEAE;
}
.inpMs .im-item .ii-left {
      width: 24%;
      height: 80rpx;
      line-height: 80rpx;
      text-align: center;
      font-size: 26rpx;
}
.inpMs .im-item .ii-right {
      width: 76%;
      height: 80rpx;
}
.inpMs .im-item .ii-right input {
        width: 100%;
        height: 100%;
        font-size: 26rpx;
}
.inpMs .im-item .item {
      width: 20%;
      height: 60rpx;
      text-align: center;
      line-height: 60rpx;
      font-size: 24rpx;
      color: #7C7C7C;
}
.inpMs .im-item .ii-getCode {
      width: 36%;
      height: 80rpx;
      position: absolute;
      top: 0;
      right: 0;
      font-size: 26rpx;
      color: #F88114;
      z-index: 99;
      line-height: 80rpx;
      text-align: center;
}
.inpMs .im-item .ii-gotCode {
      color: #666666;
}
