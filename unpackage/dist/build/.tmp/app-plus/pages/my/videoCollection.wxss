.content {
  margin-top: 20rpx;
  margin-left: 10rpx;
  float: left;
  width: 360rpx;
  height: 620rpx;
  border-radius: 15rpx;
  background-color: white;
}
.content .xuan {
    position: absolute;
    margin-top: 0rpx;
    margin-left: 330rpx;
    width: 30rpx;
    height: 30rpx;
    z-index: 100;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
}
.content .cur {
    background-image: url(data:image/png;base64,);
}
.content .img {
    width: 360rpx;
    height: 460rpx;
}
.content .row1 {
    width: 360rpx;
    height: 65rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    font-size: 28rpx;
}
.content .row1 .head {
      margin-left: 20rpx;
      margin-right: 10rpx;
      width: 50rpx;
      height: 50rpx;
}
.content .row1 .like {
      position: relative;
      left: 170rpx;
      width: 35rpx;
      height: 35rpx;
}
.content .row2 {
    font-size: 30rpx;
    margin-left: 20rpx;
}
.content .row3 {
    color: gray;
    font-size: 28rpx;
    margin-left: 20rpx;
}
.content .info {
    width: 700rpx;
    height: 220rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
}
.content .info .xuan {
      width: 30rpx;
      height: 30rpx;
      background-image: url(data:image/png;base64,);
      background-size: 100% 100%;
}
.foot {
  z-index: 1000;
  position: fixed;
  bottom: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 100%;
  height: 100rpx;
  font-size: 30rpx;
  background-color: white;
}
.foot .xuan {
    margin-left: 30rpx;
    width: 30rpx;
    height: 30rpx;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
}
.foot text {
    margin-left: 30rpx;
}
.foot .queding {
    margin-left: 360rpx;
    width: 200rpx;
    height: 75rpx;
    color: white;
    text-align: center;
    line-height: 75rpx;
    border-radius: 60rpx;
    background-color: #fc8a18;
}

