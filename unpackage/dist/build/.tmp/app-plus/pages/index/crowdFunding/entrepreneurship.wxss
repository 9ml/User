page {
  background-color: #F6F6F6;
}
.topTypeMs {
  width: 100%;
  height: 90rpx;
  background-color: #FFFFFF;
  border-top: 1rpx solid #F6F6F6;
  border-bottom: 10rpx solid #F6F6F6;
  position: fixed;
  left: 0;
  top: 120rpx;
  z-index: 9;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.topTypeMs .ttm-item {
    width: 25%;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 28rpx;
    text-align: center;
}
.topTypeMs .ttm-ac {
    color: #F77F0C;
}
.topTypeMsSpace {
  height: 101rpx;
}
.itemMs {
  width: 94%;
  height: 240rpx;
  background-color: #FFFFFF;
  border-radius: 10rpx;
  margin: 0 auto 10rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.itemMs .im-image {
    width: 28%;
    text-align: center;
    height: 190rpx;
}
.itemMs .im-image image {
      width: 150rpx;
      height: 190rpx;
}
.itemMs .im-main {
    width: 68%;
    height: 190rpx;
    margin-left: 10rpx;
}
.itemMs .im-main .imm-title {
      height: 60rpx;
      line-height: 60rpx;
      font-size: 26rpx;
}
.itemMs .im-main .imm-progress {
      height: 50rpx;
      line-height: 50rpx;
}
.itemMs .im-main .imm-info {
      height: 80rpx;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
}
.itemMs .im-main .imm-info .immi-item {
        width: 33.3%;
        height: 80rpx;
}
.itemMs .im-main .imm-info .immi-item view {
          height: 40rpx;
          line-height: 40rpx;
          font-size: 26rpx;
          text-align: center;
}
.itemMs .im-main .imm-info .immi-item .imi-top {
          color: #888888;
}
.itemMs .im-main .imm-info .immi-item .imi-bom {
          color: #F77F0C;
}

